{
  "name": "starter",
  "version": "1.0.0",
  "description": "This is a starterpack for next and asp.net core 5",
  "author": "Mads Bruun Jensen",
  "license": "MIT",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "scripts": {
    "setup": "yarn run install-dependencies",
    "install-dependencies": "yarn run install-ui-packages",
    "install-ui-packages": "cd client && yarn install",
    "serve-dev:ui-local": "cd client && yarn run dev"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "concurrently": "^5.3.0",
    "lint-staged": "^10.5.4",
    "husky": "4.3.6",
    "prettier": "^2.2.1"
  }
}
